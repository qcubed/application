{
  "name": "qcubed/application",
  "description": "The common files used by many of the components of the QCubed framework",
  "keywords": ["php", "framework", "jquery"],
  "homepage": "http://qcu.be/",
  "type": "qcubed-library",
  "license": "MIT",
  "support": {
      "issues": "https://github.com/qcubed/qcubed/issues",
      "wiki": "https://github.com/qcubed/qcubed/wiki",
      "source": "https://github.com/qcubed/common"
  },
  "require": {
      "php": ">=5.5.0",
    "qcubed/composer": "dev-qcubed4",
    "qcubed/cache": "dev-master",
    "qcubed/i18n": "dev-master",
    "qcubed/orm":"dev-master",
    "qcubed/common":"dev-master"
  },
  "minimum-stability": "dev",
  "require-dev" : {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "QCubed\\": "src/",
      "QCubed\\Codegen\\Generator\\":"codegen/generator/"
    }
  }
}
