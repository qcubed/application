{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/qcubed/common"
    },
    {
      "type": "vcs",
      "url": "https://github.com/qcubed/i18n"
    },
    {
      "type": "vcs",
      "url": "https://github.com/qcubed/orm"
    },
    {
      "type": "vcs",
      "url": "https://github.com/qcubed/cache"
    },
    {
      "type": "vcs",
      "url": "https://github.com/qcubed/application"
    }
  ],

  "name": "qcubed/application",
  "description": "The common files used by many of the components of the QCubed framework",
  "keywords": ["php", "framework", "jquery"],
  "homepage": "http://qcu.be/",
  "type": "qcubed-library",
  "license": "MIT",
  "support": {
      "issues": "https://github.com/qcubed/qcubed/issues",
      "wiki": "https://github.com/qcubed/qcubed/wiki",
      "source": "https://github.com/qcubed/common"
  },
  "require": {
      "php": ">=5.5.0",
    "qcubed/i18n": "master-dev",
    "qcubed/common":"master-dev",
    "qcubed/orm":"master-dev",
    "qcubed/composer": "master-dev",
    "qcubed/cache": "master-dev"

  },
  "minimum-stability": "dev",
  "require-dev" : {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "QCubed\\": "src/",
      "QCubed\\Codegen\\Generator":"codegen/generator/"
    }
  }
}
